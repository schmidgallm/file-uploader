{
  "name": "file-uploader",
  "version": "1.0.0",
  "description": "File uploader using react, node, cloudinary, and multer",
  "main": "index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server/index.js",
    "start:dev": "concurrently \"nodemon --ignore 'public/*'\" \"npm run public\"",
    "public": "cd public && npm run start",
    "install": "cd public && npm install",
    "build": "cd public && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schmidgallm/file-uploader.git"
  },
  "keywords": [
    "Files",
    "uploading",
    "react",
    "node",
    "cloudinary",
    "multer",
    "express"
  ],
  "author": "Michael Schmidgall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/schmidgallm/file-uploader/issues"
  },
  "homepage": "https://github.com/schmidgallm/file-uploader#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "cloudinary": "^1.22.0",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "prettier": "^1.19.1"
  }
}
